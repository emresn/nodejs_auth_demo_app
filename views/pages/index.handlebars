

<div class="card">
    <h5 class="card-header">Registered Users</h5>
    <div class="card-group">
        <ul class="list-group">

            {{#each users}}
                <li class="list-group-item">{{username}}</li>
            {{else}}
                <div class="alert alert-info"> There is no registered users !</div>
            {{/each}}
        </ul>
    </div>
    
    
</div>
<br>
    {{#if user}}
    <h4 class="m-2">Active User: {{user.username}}</h4>
    {{/if}}